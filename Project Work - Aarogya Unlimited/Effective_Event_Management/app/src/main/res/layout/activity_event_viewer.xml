<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventViewerActivity">

    <TextView
        android:id="@+id/patient_name_textview"
        android:layout_width="165dp"
        android:layout_height="39dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:hint="Event Name"
        android:singleLine="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/patient_userid_textview"
        android:layout_width="163dp"
        android:layout_height="50dp"
        android:layout_marginTop="12dp"
        android:hint="Event Topic"
        android:singleLine="false"
        app:layout_constraintEnd_toEndOf="@+id/patient_name_textview"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/patient_name_textview"
        app:layout_constraintTop_toBottomOf="@+id/patient_name_textview" />

    <Button
        android:id="@+id/prev"
        android:layout_width="86dp"
        android:layout_height="41dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="8dp"
        android:singleLine="false"
        android:text="Previous"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/event_org_comp_tv"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="@+id/doctor_specialization_textview"
        app:layout_constraintTop_toBottomOf="@+id/event_org_comp_tv"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/next"
        android:layout_width="64dp"
        android:layout_height="43dp"
        android:layout_marginStart="36dp"
        android:singleLine="false"
        android:text="Next"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/prev"
        app:layout_constraintEnd_toEndOf="@+id/teamtypetv"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/prev"
        app:layout_constraintTop_toTopOf="@+id/prev" />

    <TextView
        android:id="@+id/event_location_textview"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:hint="Event Location"
        android:singleLine="false"
        app:layout_constraintBottom_toBottomOf="@+id/event_org_comp_tv"
        app:layout_constraintEnd_toEndOf="@+id/teamtypetv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/teamtypetv"
        app:layout_constraintTop_toTopOf="@+id/event_org_comp_tv"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/patient_phone_textview"
        android:layout_width="152dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:hint="Event Head"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctor_city_textview" />

    <TextView
        android:id="@+id/doctor_email_textview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:hint="Event Head Phone"
        app:layout_constraintBottom_toBottomOf="@+id/patient_phone_textview"
        app:layout_constraintEnd_toEndOf="@+id/event_to_time_textview"
        app:layout_constraintStart_toStartOf="@+id/event_from_time_textview"
        app:layout_constraintTop_toTopOf="@+id/patient_phone_textview"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/doctor_clinic_name_textview"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:hint="Event Member 1 Name"
        app:layout_constraintEnd_toEndOf="@+id/patient_phone_textview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/patient_phone_textview"
        app:layout_constraintTop_toBottomOf="@+id/patient_phone_textview" />

    <TextView
        android:id="@+id/mem1phonetv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:hint="Event Member 1 Phone"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_clinic_name_textview"
        app:layout_constraintEnd_toEndOf="@+id/doctor_email_textview"
        app:layout_constraintStart_toStartOf="@+id/doctor_email_textview"
        app:layout_constraintTop_toTopOf="@+id/doctor_clinic_name_textview" />

    <TextView
        android:id="@+id/doctor_work_address_textview"
        android:layout_width="0dp"
        android:layout_height="39dp"
        android:layout_marginTop="8dp"
        android:hint="Event Member 2 Name"
        app:layout_constraintEnd_toEndOf="@+id/doctor_clinic_name_textview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doctor_clinic_name_textview"
        app:layout_constraintTop_toBottomOf="@+id/doctor_clinic_name_textview" />

    <TextView
        android:id="@+id/mem2phonetv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:hint="Event Member 2 Phone"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_work_address_textview"
        app:layout_constraintEnd_toEndOf="@+id/mem1phonetv"
        app:layout_constraintStart_toStartOf="@+id/mem1phonetv"
        app:layout_constraintTop_toTopOf="@+id/doctor_work_address_textview" />

    <TextView
        android:id="@+id/doctor_specialization_textview"
        android:layout_width="152dp"
        android:layout_height="26dp"
        android:layout_marginTop="8dp"
        android:hint="Event Fees"
        app:layout_constraintEnd_toEndOf="@+id/doctor_work_address_textview"
        app:layout_constraintStart_toStartOf="@+id/doctor_work_address_textview"
        app:layout_constraintTop_toBottomOf="@+id/doctor_work_address_textview" />

    <TextView
        android:id="@+id/teamtypetv"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:hint="Event Team Type"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_specialization_textview"
        app:layout_constraintEnd_toEndOf="@+id/mem2phonetv"
        app:layout_constraintStart_toStartOf="@+id/mem2phonetv"
        app:layout_constraintTop_toTopOf="@+id/doctor_specialization_textview" />

    <ImageView
        android:id="@+id/medicine_imageview"
        android:layout_width="0dp"
        android:layout_height="127dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_password_textview"
        app:layout_constraintEnd_toStartOf="@+id/patient_name_textview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/patient_name_textview"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:color/holo_blue_dark" />

    <TextView
        android:id="@+id/event_from_time_textview"
        android:layout_width="68dp"
        android:layout_height="36dp"
        android:hint="From Time"
        android:singleLine="false"
        app:layout_constraintBottom_toBottomOf="@+id/doctor_city_textview"
        app:layout_constraintStart_toStartOf="@+id/patient_userid_textview"
        app:layout_constraintTop_toTopOf="@+id/doctor_city_textview"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/event_to_time_textview"
        android:layout_width="84dp"
        android:layout_height="37dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:hint="To Time"
        android:singleLine="false"
        app:layout_constraintBottom_toBottomOf="@+id/event_from_time_textview"
        app:layout_constraintEnd_toEndOf="@+id/patient_userid_textview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/event_from_time_textview"
        app:layout_constraintTop_toTopOf="@+id/event_from_time_textview"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/doctor_city_textview"
        android:layout_width="124dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:hint="Event Date"
        app:layout_constraintEnd_toEndOf="@+id/medicine_imageview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doctor_password_textview" />

    <TextView
        android:id="@+id/event_org_comp_tv"
        android:layout_width="149dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:hint="Event Organizers"
        app:layout_constraintEnd_toEndOf="@+id/doctor_specialization_textview"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/doctor_specialization_textview"
        app:layout_constraintTop_toBottomOf="@+id/doctor_specialization_textview" />

    <Button
        android:id="@+id/participate_event_button"
        android:layout_width="112dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Participate"
        app:layout_constraintBottom_toBottomOf="@+id/prev"
        app:layout_constraintEnd_toStartOf="@+id/next"
        app:layout_constraintStart_toEndOf="@+id/prev"
        app:layout_constraintTop_toTopOf="@+id/prev" />

    <TextView
        android:id="@+id/doctor_password_textview"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginTop="16dp"
        android:hint="Event ID"
        app:layout_constraintEnd_toEndOf="@+id/patient_userid_textview"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/patient_userid_textview"
        app:layout_constraintTop_toBottomOf="@+id/patient_userid_textview" /><![CDATA[

    "
    android:hint="event_location_textview"
    app:layout_c
    Bottom_toTopOf="@+id/headnametextview"
    app:layout_c
    Start_toStartOf="@+id/event_imageview"
    app:layout_constrai />
]]>
</android.support.constraint.ConstraintLayout>